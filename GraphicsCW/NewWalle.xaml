        <Label Content="Trunk width" Height="28" HorizontalAlignment="Left" Margin="281,206,0,0" Name="label4" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="10" Margin="33,293,0,0" Maximum="130" Minimum="30" Name="trunkHeightSlider" SmallChange="10" TickFrequency="10" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Trunk height" Height="28" HorizontalAlignment="Left" Margin="281,263,0,0" Name="label5" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="5" Margin="33,357,0,0" Maximum="90" Minimum="30" Name="neckHeightSlider" SmallChange="5" TickFrequency="5" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Neck height" Height="28" HorizontalAlignment="Left" Margin="281,327,0,0" Name="label6" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="5" Margin="33,416,0,0" Maximum="80" Minimum="20" Name="neckWidthSlider" SmallChange="5" TickFrequency="5" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Neck width" Height="28" HorizontalAlignment="Left" Margin="281,386,0,0" Name="label7" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="1" Margin="33,476,0,0" Maximum="25" Minimum="10" Name="trackRadiusSlider" SmallChange="1" TickFrequency="1" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Track radius" HorizontalAlignment="Left" Margin="281,446,0,0" Name="label8" Height="28" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="1" Margin="33,539,0,0" Maximum="30" Minimum="15" Name="eyeHeightSlider" SmallChange="1" TickFrequency="1" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Eye height" Height="28" HorizontalAlignment="Left" Margin="281,509,0,0" Name="label9" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="704,50,0,0" Name="panellsNum" VerticalAlignment="Top" Width="149">
            <ComboBoxItem Content="0" IsSelected="True" />
            <ComboBoxItem Content="1" />
            <ComboBoxItem Content="2" />
            <ComboBoxItem Content="3" />
        </ComboBox>
        <Label Content="Panells number" Height="28" HorizontalAlignment="Left" Margin="732,20,0,0" Name="label10" VerticalAlignment="Top" />
