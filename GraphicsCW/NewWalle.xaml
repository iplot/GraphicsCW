                <Button Content="Hands" Height="23" HorizontalAlignment="Left" Margin="6,57,0,0" Name="handsColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="99,54,0,0" Name="handsColorText" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
                <Button Content="Trunk" Height="23" HorizontalAlignment="Left" Margin="6,102,0,0" Name="trunkColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="99,99,0,0" Name="trunkColorText" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
                <Button Content="Neck" Height="23" HorizontalAlignment="Left" Margin="6,151,0,0" Name="neckColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="99,148,0,0" Name="neckColorText" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
                <Button Content="Eye" Height="23" HorizontalAlignment="Left" Margin="6,197,0,0" Name="eyeColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="99,194,0,0" Name="eyeColorText" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
                <Button Content="Track" Height="23" HorizontalAlignment="Left" Margin="6,243,0,0" Name="trackColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="99,240,0,0" Name="trackColorText" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
                <Button Content="Panells" Height="23" HorizontalAlignment="Left" Margin="6,288,0,0" Name="panellsColor" VerticalAlignment="Top" Width="75" Click="housingCoor_Click" />
                <TextBox Height="23" HorizontalAlignment="Left" IsReadOnly="True" Margin="99,285,0,0" Name="panellsColorText" VerticalAlignment="Top" Width="155" />
            </Grid>
        </GroupBox>
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="732,582,0,0" Name="okButton" VerticalAlignment="Top" Width="75" Click="okButton_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="837,582,0,0" Name="cancelBut" VerticalAlignment="Top" Width="75" Click="cancelBut_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="652,489,0,0" Name="baseX" VerticalAlignment="Top" Width="52" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="837,489,0,0" Name="baseZ" VerticalAlignment="Top" Width="52" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="745,489,0,0" Name="baseY" VerticalAlignment="Top" Width="52" />
        <Label Content="X" Height="28" HorizontalAlignment="Left" Margin="668,459,0,0" Name="label11" VerticalAlignment="Top" />
        <Label Content="Y" Height="28" HorizontalAlignment="Left" Margin="762,459,0,0" Name="label12" VerticalAlignment="Top" />
        <Label Content="Z" Height="28" HorizontalAlignment="Left" Margin="853,459,0,0" Name="label13" VerticalAlignment="Top" />
