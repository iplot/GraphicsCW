<Window x:Class="GraphicsCW.NewWalle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewWalle" Height="656" Width="946">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,50,0,0" Name="housingLengthText" VerticalAlignment="Top" Width="120" Text="50" TextAlignment="Center" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,114,0,0" Name="handsLengthText" Text="40" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,175,0,0" Name="handsWidthText" Text="10" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,236,0,0" Name="trunkWidthText" Text="30" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,293,0,0" Name="trunkHeightText" Text="30" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,357,0,0" Name="neckHeightText" Text="30" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,416,0,0" Name="neckWidthText" Text="20" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,476,0,0" Name="trackRadiusText" Text="10" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="483,539,0,0" Name="eyeHeightText" Text="15" TextAlignment="Center" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        
        
        <Slider Height="23" HorizontalAlignment="Left" Margin="33,50,0,0" Name="housingLengthSlider" VerticalAlignment="Top" Width="419" Minimum="50" Maximum="150" SmallChange="5" LargeChange="5" TickPlacement="BottomRight" AutoToolTipPlacement="None" AutoToolTipPrecision="0" IsSnapToTickEnabled="True" TickFrequency="5" IsSelectionRangeEnabled="True" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Housing length" Height="28" HorizontalAlignment="Left" Margin="281,20,0,0" Name="label1" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="5" Margin="33,114,0,0" Maximum="100" Minimum="40" Name="handsLengthSlider" SmallChange="5" TickFrequency="5" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Hands length" Height="28" HorizontalAlignment="Left" Margin="281,84,0,0" Name="label2" VerticalAlignment="Top" />
        <Slider AutoToolTipPlacement="None" AutoToolTipPrecision="0" Height="23" HorizontalAlignment="Left" IsSelectionRangeEnabled="True" IsSnapToTickEnabled="True" LargeChange="1" Margin="33,175,0,0" Maximum="30" Minimum="10" Name="handsWidthSlider" SmallChange="1" TickFrequency="1" TickPlacement="BottomRight" VerticalAlignment="Top" Width="419" ValueChanged="housingLengthSlider_ValueChanged" />
        
        <Label Content="Hands width" Height="28" HorizontalAlignment="Left" Margin="281,145,0,0" Name="label3" VerticalAlignment="Top" />
